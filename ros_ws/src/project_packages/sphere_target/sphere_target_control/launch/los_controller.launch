<launch>
    <node pkg="sphere_target_control" type="simple_los.py" name="controller" ns="sphere_target" output="screen">
        <param name="use_accel_fb" value="false"/>
        <rosparam command="load" file="$(find sphere_target_control)/config/controllers/cascade/inertial.yaml"/>
        <rosparam command="load" file="$(find sphere_target_control)/config/controllers/cascade/simple_los.yaml"/>
        <remap from="odometry/feedback" to="odometry/filtered"/>
        <remap from="accel/feedback" to="accel/filtered"/>
        <remap from="range" to="mavros/distance_sensor/rangefinder_pub"/>
    </node>
    <node pkg="rqt_reconfigure" type="rqt_reconfigure" name="dyn_reconfigure"/>
</launch>